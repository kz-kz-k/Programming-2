#include <stdio.h>


int main()
{
    int a[10],i,n,sum=0;

    printf("Enter size of the array : ");
    scanf("%d",&n);

    for(i=0; i<n; i++)
    {
        printf(" Elements  : ");
        scanf("%d",&a[i]);
    }


    for(i=0; i<n; i++)
    {

        sum+=a[i];
    }

     printf("Sum of all array elements: %d",sum);

    return 0;
}
